    "recursive-readdir-sync": "^1.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@commitlint/prompt-cli": "^8.2.0",
    "ava": "2.4.0",
    "coveralls": "^3.0.6",
